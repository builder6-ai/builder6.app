{
  "name": "@builder6/root",
  "version": "0.0.1",
  "description": "Builder6 Monorepo",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --filter @builder6/runtime dev",
    "start": "pnpm --filter @builder6/runtime start",
    "start:prod": "pnpm --filter @builder6/runtime start:prod",
    "format": "prettier --write \"packages/**/*.ts\"",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "test:watch": "pnpm -r test:watch",
    "test:cov": "pnpm -r test:cov",
    "clean": "pnpm -r clean && rm -rf node_modules"
  },
  "dependencies": {},
  "devDependencies": {
    "prettier": "^3.4.2",
    "typescript": "^5.7.3"
  }
}
